{
  "functions": {
    "api/vercel.php": {
      "runtime": "vercel-php@0.7.3",
      "maxDuration": 22
    }
  },
  "rewrites": [
    { "source": "/public/:path*", "destination": "/:path*" },
    { "source": "/manifest", "destination": "/api/vercel.php" },
    { "source": "/api/", "destination": "/api/vercel.php?json=1" },
    { "source": "/", "destination": "/api/vercel.php" }
  ]
}
